generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}
model Post{
  id String @id @default(uuid())
  user String
  content String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  
}
model Like {
  id String @id @default(uuid())

  post   Post   @relation(fields: [postId], references: [id])
  postId String

  likedBy String
}